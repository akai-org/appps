---
import { octokit } from "../utilities/github";

const per_page = 3;
const res = await octokit.request(`GET /orgs/{org}/repos?per_page=${per_page}`, {
  org: 'akai-org',
  headers: {
    'X-GitHub-Api-Version': '2022-11-28'
  }
});
console.log(res);
